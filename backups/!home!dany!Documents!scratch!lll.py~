class Node(object):
    def __init__(self, val, next_node):
        self.value = val
        self.next_node = next_node

class LinkedList(object):
    def __init__(self, rand_access_list):
        self.rand_access_list = rand_access_list
        self.ll = []
        for index in range(len(self.rand_access_list)):
            if index = len(self.rand_access_list):
                next_ind = -1
            else:
                next_ind = index+1
            self.ll.append(Node(self.rand_access_list[index]
