mylist = [1, 3, 1, 5, 1, 1, 4, 1, 1, 1, 4, 1, 2, 1]

def dutch(v, start, end):
    """
    In palce Dutch Flag Partitioning
    """
    if end-start <= 1:
        return [v[start]]  # do nothing
    piv_i = (start + end)/2
    piv_end = piv_i
    piv = v[piv_i]
    left = start
    right = end-1
    while (left < piv_i or right > piv_end) and left < right:
        while left < piv_i and v[left] <= piv:
            if v[left] == piv:
                piv_i -= 1
                v[left] = v[piv_i]
                v[piv_i] = piv
            elif v[left] < piv:
                left += 1
        while right < piv_end and v[right] >= piv:
            if v[right] == piv:
                piv_end += 1
                v[right] = v[piv_end]
                v[piv_end] = piv
            elif v[right] < piv:
                right += 1
        if left == piv_i:
            piv_i += 1
        if right == piv_end:
            piv_end -= 1
        tmp = v[left]
        v[left] = v[right]
        v[right] = tmp
        left += 1
        right -= 1
